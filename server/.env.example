# Thông tin ứng dụng
PROJECT_NAME="AI Agent Giải Thuật"
DEV_MODE=true

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# Database
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=giaithuat

# Security
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Google API & AI Models
GOOGLE_API_KEY=your_google_api_key
AGENT_LLM_MODEL=gemini-2.5-flash
CREATIVE_LLM_MODEL=gemini-2.5-pro
EMBEDDING_MODEL=text-embedding-004

# Vector Database
PINECONE_API_KEY=your_pinecone_api_key
MONGO_URI=your_mongo_uri

# LangSmith (optional)
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=default

# File Upload
UPLOAD_DIR=uploads

# Cookie settings
COOKIE_DOMAIN=""
COOKIE_SECURE=false
COOKIE_SAMESITE=lax
COOKIE_NAME=access_token

# Migration & Seeder
RUN_MIGRATIONS_ON_STARTUP=false
RUN_SEEDERS_ON_STARTUP=false

# S3/R2 Settings (optional)
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_REGION=auto
S3_BUCKET_NAME=giaithuat
S3_ENDPOINT_URL=http://minio:9000
S3_PUBLIC_URL=http://localhost:9000

# Prefixes cho các loại file
S3_COURSE_IMAGE_PREFIX=course-images/
S3_USER_AVATAR_PREFIX=user-avatars/
S3_DOCUMENT_PREFIX=documents/

# Document Processing Settings
DOCUMENT_PROCESSING_ENDPOINT=https://api.runpod.ai/v2/e4c6c468on2q2w/run
RUNPOD_API_KEY=
DOCUMENT_PROCESSING_TIMEOUT=300
JUDGE0_API_URL=https://api.judge0.com